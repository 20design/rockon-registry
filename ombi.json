{
    "Ombi": {
        "website": "https://hub.docker.com/r/rogueosb/ombi/",
        "version": "1.0",
        "description": "Ombi is a request webui for Plex. It supports Sonarr, Radarr, Sickbeard & friends, along with couchpotato.",
        "containers": {
            "ombi": {
                "image": "rogueosb/ombi",
                "launch_order": 1,
                "volume_add_support": true,
                "ports": {
                    "3579": {
                        "description": "Ombi WebUI port. Suggested default: 3579",
                        "host_default": 3579,
                        "label": "WebUI port",
                        "protocol": "tcp",
                        "ui": true
                    }
                },
                "volumes": {
                    "/config": {
                        "description": "Choose a Share for Ombi Configuration Files",
                        "label": "Config Storage"
                    }
                },
                "environment": {
                    "EAP": {
                        "description": "Enter a 1 to enable early access preview.",
                        "label": "Enable early access preview.",
                        "index": 1
                    }
                }
            }
        }
    }
}
